{
  "name": "things",
  "version": "0.1.0",
  "description": "A GTD Tool",
  "engines": {
    "node": ">=7"
  },
  "author": "songjia.happycoding@gmail.com",
  "scripts": {
    "client-dev-server": "cross-env DEBUG=app:* node client-react/bin/dev-server.js",
    "build": "node build/build.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "cross-env NODE_ENV=test nyc mocha --harmony-async-await test/server/**/*.js",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage": "nyc report --reporter=lcov"
  },
  "dependencies": {
    "antd": "^2.5.3",
    "bcrypt-nodejs": "0.0.3",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-jwt": "^1.2.0",
    "koa-router": "^7.0.1",
    "monk": "^3.0.4",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.0.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.2.1",
    "redbox-react": "^1.3.3",
    "sass-loader": "^4.1.1",
    "shelljs": "^0.7.5",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "nyc": {
    "include": [
      "server/**/*.js"
    ],
    "exclude": [
      "server/config.js"
    ],
    "report-dir": "./test/coverage",
    "all": true,
    "cache": false
